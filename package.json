{
  "name": "polymarbot-website",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "dotenv -c -- nuxt build",
    "dev": "dotenv -c -- nuxt dev",
    "generate": "nuxt generate",
    "preview": "dotenv -c -- nuxt preview",
    "postinstall": "nuxt prepare && husky",
    "depcheck": "knip",
    "typecheck": "nuxt typecheck",
    "lint": "git add . && lint-staged --config .lintstagedrc.lint-only.json && pnpm typecheck",
    "format": "git add . && lint-staged",
    "format:file": "eslint --fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "i18n:build": "tsx scripts/i18n/build && pnpm i18n:fix:alignment",
    "i18n:check": "tsx scripts/i18n/build && pnpm i18n:check:undefined && pnpm i18n:check:unused && pnpm i18n:check:alignment",
    "i18n:check:undefined": "tsx scripts/i18n/checkUndefinedKeys",
    "i18n:check:unused": "tsx scripts/i18n/checkUnusedKeys",
    "i18n:check:alignment": "tsx scripts/i18n/checkI18nAlignment.ts",
    "i18n:fix": "pnpm i18n:fix:unused && pnpm i18n:fix:alignment",
    "i18n:fix:unused": "tsx scripts/i18n/checkUnusedKeys --fix && tsx scripts/i18n/build",
    "i18n:fix:alignment": "tsx scripts/i18n/checkI18nAlignment.ts --fix",
    "db:generate": "nuxt prepare && pnpm db:app:generate && pnpm db:bot:generate && pnpm db:logs:generate",
    "db:app:generate": "prisma generate --config=prisma/app/prisma.config.ts",
    "db:app:push": "prisma db push --config=prisma/app/prisma.config.ts",
    "db:app:studio": "prisma studio --config=prisma/app/prisma.config.ts",
    "db:bot:generate": "prisma generate --config=prisma/bot/prisma.config.ts",
    "db:bot:pull": "prisma db pull --config=prisma/bot/prisma.config.ts",
    "db:bot:studio": "prisma studio --config=prisma/bot/prisma.config.ts",
    "db:logs:generate": "prisma generate --config=prisma/bot_logs/prisma.config.ts",
    "db:logs:pull": "prisma db pull --config=prisma/bot_logs/prisma.config.ts",
    "db:logs:studio": "prisma studio --config=prisma/bot_logs/prisma.config.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "@polymarbot/shared": "^0.4.0",
    "@polymarket/builder-relayer-client": "^0.0.8",
    "@primeuix/themes": "^2.0.3",
    "@primevue/nuxt-module": "^4.5.4",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/client-runtime-utils": "^7.3.0",
    "@sentry/vue": "^10.36.0",
    "better-auth": "^1.4.17",
    "clsx": "^2.1.1",
    "countup.js": "^2.9.0",
    "dayjs": "^1.11.19",
    "echarts": "^6.0.0",
    "lodash-es": "^4.17.23",
    "nuxt": "^4.3.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "qrcode": "^1.5.4",
    "resend": "^6.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-primeui": "^0.6.1",
    "typed.js": "^3.0.0",
    "viem": "^2.45.0",
    "vue": "^3.5.27",
    "vue-echarts": "^8.0.1",
    "vue-gtag": "^3.6.3",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@bjj/eslint-config-shared": "^0.1.3",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/image": "^2.0.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/robots": "^5.6.7",
    "@nuxtjs/sitemap": "^7.5.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.10",
    "@types/qrcode": "^1.5.6",
    "@vitest/ui": "^4.0.18",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "nuxt-security": "^2.5.1",
    "prisma": "^7.3.0",
    "sass": "^1.97.3",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unplugin": "^3.0.0",
    "unplugin-vue-markdown": "^29.2.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "@noble/hashes": "1.8.0"
    }
  },
  "packageManager": "pnpm@10.14.0"
}
